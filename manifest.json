{
    "manifest_version": 2,

    "name": "UZIP",
    "description": "Url Shortner",
    "version": "1.0",

    "permissions": [
        "tabs",
        "clipboardWrite",
        "clipboardRead"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html",
        "default_title": "UZIP - url shortner"
    }
}
